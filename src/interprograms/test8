 class hola{
 int ackermann(int m, int n){
    int r;
    if (!m){
        r = n + 1;
    }
     if (!n){
      r = ackermann(m - 1, 1);
     }
     else{
        r = ackermann(m - 1, ackermann(m, n - 1));
     }
     return r;
 }
 void main()
 {
         int m, n;
         for (m = 0; m <= 4; m++)
                 for (n = 0; n < 6 - m; n++)
                         printf("A(%d, %d) = %d\n", m, n, ackermann(m, n));

         return 0;
 }
 }